<a href="/">Go Back</a>
<h2>Celebrities</h2>
<ul>
  {{#each celebrities}}
  <li><a href="/celebrities/{{this._id}}">{{this.name}}</a>
    <form method="post" action="celebrities/{{this._id}}/delete">
      <button>Delete</button>
    </form>
  </li>
  {{/each}}
</ul>

<a href="/celebrities/new">Create a celebrity</a>